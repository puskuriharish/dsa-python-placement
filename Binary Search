def binary_search(arr,key):
    left ,right = 0,len(arr)-1
    while left <= right:
        mid = (left+right)//2
        if arr[mid]==key:
            return mid
        elif key <arr[mid]:
            right = mid-1
        else:
            left = mid+1
arr = [2,3,5,6,7,8,9,10,13,15]
key = 7
print(binary_search(arr,key))
