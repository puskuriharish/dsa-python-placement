def mini_maxi(arr,key):
    mymin = arr[0]
    mymax = arr[0]
    for i in range(1,len(arr)):
        if arr[i] < mymin:
            mymin = arr[i]
        elif arr[i] > mymax:
            mymax = arr[i]
    return f"Minimum :{mymin} Maximum :{mymax}"
        
        
arr = [1,7,3,2,6,9,4]
key = 4
print(mini_maxi(arr,key))

# Output : Minimum :1 Maximum :9
